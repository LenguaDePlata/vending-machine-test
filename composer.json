{
    "name": "inaki/vending-machine-test",
    "description": "Technical challenge",
    "type": "project",
    "config": {
        "optimize-autoloader": true
    },
    "require": {
        "php-di/php-di": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "App/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ],
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1"
    }
}
